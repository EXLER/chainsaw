{
  "name": "pcss-framework",
  "version": "1.1.1",
  "description": "Instant mobile-friendly CSS framework for prototyping.",
  "keywords": [
    "css",
    "framework",
    "css-framework",
    "scss-framework",
    "sass-framework",
    "bootstrap"
  ],
  "author": "EXLER",
  "repository": {
    "type": "git",
    "url": "https://github.com/EXLER/pcss.git"
  },
  "scripts": {
    "compile": "node-sass ./src/pcss.scss ./dist/pcss.css",
    "minify": "minify ./dist/pcss.css > ./dist/pcss.min.css",
    "build": "npm run compile && npm run minify"
  },
  "devDependencies": {
    "minify": "^5.1.1",
    "node-sass": "^4.14.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}